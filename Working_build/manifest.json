{
  "name": "ExtJS Component Inspector",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Chrome extension to inspect elements on a page; similar to Chrome DevTools",
  "devtools_page": "/source/sidebarPanel/index.html",
  "permissions": ["devtools", "scripting"],
  "minimum_chrome_version": "69",
  "host_permissions": ["scripting"],
  "sandbox": {
    "pages": ["/source/sidebarPanel/app.html"]
  }
}
